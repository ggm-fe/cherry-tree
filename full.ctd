						<rich_text foreground="#a5a52a2a2a2a"> --&gt;
    &lt;g&gt;
      &lt;circle fill=&quot;url(</rich_text>
						<rich_text foreground="#a0a02020f0f0">#g1</rich_text>
						<rich_text foreground="#a5a52a2a2a2a">)&quot; r=&quot;50&quot; id=&quot;</rich_text>
						<rich_text foreground="#a0a02020f0f0">sun</rich_text>
						<rich_text foreground="#a5a52a2a2a2a">&quot;/&gt;
      &lt;rect width=&quot;200&quot; height=&quot;70&quot; id=&quot;</rich_text>
						<rich_text foreground="#a0a02020f0f0">rect</rich_text>
						<rich_text foreground="#a5a52a2a2a2a">&quot; fill=&quot;url(</rich_text>
						<rich_text foreground="#a0a02020f0f0">#g2</rich_text>
						<rich_text foreground="#a5a52a2a2a2a">)&quot;/&gt;
    &lt;/g&gt;
  &lt;/</rich_text>
